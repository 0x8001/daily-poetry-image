---
import ImageCard from './ImageCard.astro';
import data from "../assets/list.json"
---

<div class="container mx-auto px-4 pt-16 mb-16">
	<div>
		<h2 class="tracking-wider text-orange-500 text-lg font-semibold">All Images</h2>
		<div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-8">
			{data.map((item) => <ImageCard item={item} />)}
		</div>
	</div>
</div>
